<script>
  $(function(){
    $('section.feature-slider').each(function(){
      if ($('.text .step', this).length > 1) {
        var arrow = (_sf.lang == 'ar') ? '←' : '→';
        $('<a href="#" class="more">más ' + arrow + '</a>').appendTo(this);
      }
    }).on('click', '.more', function(evt){
      var $box = $(this).closest('.feature-slider');
      if(!$box[0])
        return;

      var $cImg = $('.img .current', $box),
          $cText = $('.text .current', $box);

      if(!$cImg[0] || !$cText[0])
        return;
      if($('.img picture', $box)[0]) {
        var $nImg = $cImg.next('.img picture');
      } else {
        var $nImg = $cImg.next('.img img');
      }
      var $nText = $cText.next('.step');

      if(!$nImg[0]) {
        if($('.img picture', $box)[0]) {
          $nImg = $('.img picture', $box).first();
        } else {
          $nImg = $('.img img', $box).first();
        }
      }

      if(!$nText[0])
        $nText = $('.step', $box).first();

      if(!$nImg[0] || !$nText[0])
        return;

      swap($cImg, $nImg);
      swap($cText, $nText);
      return false;
    });

    function swap($cur, $next) {
      $cur.removeClass('current');
      $next.addClass('current');
    }
  });
</script>
